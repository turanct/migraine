{
    "name": "engagor/migraine",
    "type": "library",
    "description": "",
    "keywords": [
        "database",
        "migrations"
    ],
    "homepage": "https://github.com/engagor/migraine",
    "license": "MIT",
    "authors": [
        {
            "name": "Toon Daelman",
            "email": "spinnewebber_toon@hotmail.com",
            "homepage": "https://github.com/turanct",
            "role": "Developer"
        },
        {
            "name": "Ben Serlippens",
            "email": "ben.serlippens@gmail.com",
            "homepage": "https://github.com/BenSeDev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-pdo": "*",
        "symfony/finder": "^v4.4.8",
        "symfony/console": "^v4.4.7",
        "symfony/filesystem": "^v5.2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.20 || ^9.5.2",
        "friendsofphp/php-cs-fixer": "^v2.16.3",
        "vimeo/psalm": "^3.11.2"
    },
    "autoload": {
        "psr-4": {
            "Turanct\\Migraine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Turanct\\Migraine\\": "tests"
        }
    },
    "bin": [
        "bin/migraine"
    ],
    "scripts": {
        "test": "phpunit",
        "fix": "php-cs-fixer fix src/ --rules=@PSR2",
        "check-style": "php-cs-fixer fix src/ --rules=@PSR2 --dry-run",
        "psalm": "psalm --show-info=true"
    },
    "config": {
        "sort-packages": true
    }
}
